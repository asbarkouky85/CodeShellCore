    <ProjectReference Include="$(CODESHELL_HOME)\CodeShellCore.MQ\CodeShellCore.MQ.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="appsettings.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
