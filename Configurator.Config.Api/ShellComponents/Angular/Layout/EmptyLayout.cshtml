@if (IsSectionDefined("header"))
{
    @RenderSection("header")
}
else
{
    @Html.PageHeader(Html.GetViewParams().AddUrl)
}

@RenderBody()
@RenderSection("lists", false)
@if (IsSectionDefined("buttons"))
{
    if (Html.Config().DefaultAccessibility == 2)
    {
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <div class="panel panel-default">
                        <div class="panel-body">
                            <div class="btn-group pull-last">
                                @RenderSection("buttons", false)
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    }
    else
    {
        IgnoreSection("buttons");
    }


}




