@model PageDTO
@{
    Layout = Html.Config().Layout; ;
    Html.HeaderModel().ToolsSection = Html.SelectControlGroup(d => d.TenantCode, Lister.Make("tenants", false), "name", "code", inputAttr: new { evnt__change = "tenantChanged()" },nullable:false);
    //Html.HeaderModel().AddButton = Html.GetPartial("NavigationGroups/_NavPageButton");
    Html.HeaderModel().AddToButtons(title: "Save", icon: "fa fa-save fa-lg", function: "save()", btn: BtnClass.Primary);
    Html.HeaderModel().AddToButtons(title: "Add Page To Nave", icon: "fa fa-plus fa-lg", function: "AddPage()", btn: BtnClass.Success);
}
<div class="row">
    <div class="col-md-4">
        @Html.ComponentSelectorFromOther("NaveList", "NaveList", attr: new { evnt__valueChange = "getPages($event)" })
    </div>
    <div class="col-md-8">
        @Html.ComponentSelectorFromOther("NavigationPageList", "NavigationPageList")
    </div>
</div>